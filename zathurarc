
set default-fg "#55BB77"
set default-bg "#010101"

set adjust-open "width"

set recolor true
" the dark bg and green font"
" below setting control background color "
set recolor-lightcolor "#232323"
" below setting control the font color "
set recolor-darkcolor "#77aa77"
" disable/off below setting when set above colorscheme"
" set recolor-keephue true
" map <C-R> recolor
" map <C-i> zoom in 
" map <C-o> zoom out 



set font "Sans normal 12"
set guioptions "cs"
set show-v-scrollbar true
set window-height 600
set window-width 1000

